{
  "$schema": "./package-schema.json",
  "type": "module",
  "name": "fastify-starter-standard",
  "version": "1.0.0",
  "description": "Fastify starter for Node.JS projects",
  "main": "index.js",
  "tap": {
    "check-coverage": false,
    "test-env": [
      "ESM_DISABLE_CACHE=1"
    ]
  },
  "scripts": {
    "start": "node index",
    "dev": "nodemon index",
    "test": "cross-env COVERALLS_REPO_TOKEN='' c8 --reporter text tap --no-cov || true",
    "posttest": "npm run coveralls && npm run codecov",
    "coveralls": "cross-env c8 --reporter text-lcov --report-dir coverage report | coveralls",
    "codecov": "c8 --reporter=text-lcov tap --no-coverage | codecov --disable=gcov --pipe",
    "cover": "cross-env COVERALLS_REPO_TOKEN='' c8 tap --no-cov",
    "lint": "standard",
    "lint-fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wmitrus/fastify-starter-standard.git"
  },
  "keywords": [
    "basic",
    "starter",
    "node",
    "nodejs",
    "standard",
    "standard.js"
  ],
  "author": "Wojciech Mitru≈õ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wmitrus/fastify-starter-standard/issues"
  },
  "homepage": "https://github.com/wmitrus/fastify-starter-standard#readme",
  "devDependencies": {
    "c8": "^7.11.2",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "standard": "^17.0.0-2",
    "tap": "^16.0.1"
  },
  "dependencies": {
    "@fastify/autoload": "^4.0.1",
    "@fastify/helmet": "^8.0.0",
    "@fastify/http-proxy": "^7.1.0",
    "@fastify/redis": "^5.0.0",
    "@fastify/sensible": "^4.1.0",
    "@fastify/swagger": "^6.0.0",
    "config": "^3.3.7",
    "dotenv": "^16.0.1",
    "fastify": "^3.28.0",
    "moment": "^2.29.3",
    "node-fetch": "^3.2.4"
  }
}
